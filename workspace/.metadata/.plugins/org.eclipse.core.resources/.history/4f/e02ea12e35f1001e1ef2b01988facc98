package com.ftninformatika.jwd.modul1.termin1.model;

public class Proizvod {
	private long id;
	private String naziv;
	private double cena;
	
	
	public Proizvod(long id, String naziv, double cena) {
		this.id = id;
		this.naziv = naziv;
		this.cena = cena;
	}
	public Proizvod(String naziv, double cena) {
		this(0,naziv,cena);
	}
	public Proizvod() {
		this(0,"",0.0);
	}

}
