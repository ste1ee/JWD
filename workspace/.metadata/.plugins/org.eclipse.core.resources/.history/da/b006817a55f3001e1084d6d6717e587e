package com.ftninformatika.jwd.modul1.termin3.modelovanje;

public class Stavka {
	private long id;
	private int kolicina;
	Proizvod proizvod;
	
	//konstruktori
	public Stavka(long id, int kolicina) {
		super();
		this.id = id;
		this.kolicina = kolicina;
	}
	public Stavka(int kolicina) {
		this(0, kolicina);
	}
	public Stavka() {
		this(0, 0);
	}
	
	

}
