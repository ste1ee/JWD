package com.ftninformatika.jwd.modul1.termin1.model;

public class Racun {
	private long id;
	private String datum;
	private double cena;
	
	
	//konstruktor sa atributima
	public Racun(long id, String datum, double cena) {
		this.id = id;
		this.datum = datum;
		this.cena = cena;
	}
	
	//konstruktor bez id-a
	public Racun(String datum, double cena) {
		this(0,datum,cena);
	}
	
	//konstruktor bez atributa
	public Racun() {
		this(0,null,0.0);
	}
}
